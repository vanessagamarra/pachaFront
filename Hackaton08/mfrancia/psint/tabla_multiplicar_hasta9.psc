Proceso tabla_multiplicar_hasta9
	Para i <- 1 Hasta 9 Con Paso 1 Hacer
        Escribir "Tabla de multiplicar del ", i
        
        Para j <- 1 Hasta 10 Con Paso 1 Hacer
            resultado <- i * j
            Escribir i, " x ", j, " = ", resultado
        Fin Para
        
        Escribir ""
        
    Fin Para
FinProceso
