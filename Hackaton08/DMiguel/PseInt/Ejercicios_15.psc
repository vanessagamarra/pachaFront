Proceso Ejercicios_15
	Escribir "Ingrese el dato en Cm"
	Leer centimetro
	Escribir "Ingrese el dato en Libras"
	Leer libras
	pulgadas <- centimetro*0.393701
	kilosgramos <- libras*0.453592
	Escribir "los ",centimetro," cm son en pulgadas ",pulgadas
	Escribir "las libras ",libras, " son en kilogramos ",kilosgramos
FinProceso
